{
  "name": "havasafi",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "set DARK_MODE=media & expo start --android",
    "dev": "expo start -c --android",
    "ios": "DARK_MODE=media expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "build:pre": "eas build -p android --profile preview",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "DARK_MODE=media expo start --web",
    "postinstall": "patch-package",
    "update:sdk": "expo install expo@latest && expo install --fix"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/roboto": "^0.2.3",
    "@expo-google-fonts/teko": "^0.2.3",
    "@expo/vector-icons": "^14.0.4",
    "@faker-js/faker": "^9.5.0",
    "@gluestack-ui/button": "^1.0.8",
    "@gluestack-ui/icon": "^0.1.25",
    "@gluestack-ui/input": "^0.1.32",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.16",
    "@gluestack-ui/spinner": "^0.1.15",
    "@gluestack-ui/toast": "^1.0.8",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.66.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "date-fns": "^4.1.0",
    "expo": "^52.0.35",
    "expo-camera": "~16.0.16",
    "expo-constants": "~17.0.6",
    "expo-font": "~13.0.3",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "lottie-react-native": "^7.1.0",
    "lucide-react-native": "^0.475.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "3.16.2",
    "react-native-safe-area-context": "5.2.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@types/react": "~18.3.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.1.0",
    "jscodeshift": "0.15.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "packageManager": "pnpm@10.3.0+sha512.ee592eda8815a8a293c206bb0917c4bb0ff274c50def7cbc17be05ec641fc2d1b02490ce660061356bd0d126a4d7eb2ec8830e6959fb8a447571c631d5a2442d",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@faker-js/faker",
          "@gluestack-ui/button",
          "@gluestack-ui/icon",
          "@gluestack-ui/input",
          "@gluestack-ui/nativewind-utils",
          "@gluestack-ui/overlay",
          "@gluestack-ui/spinner",
          "@gluestack-ui/toast",
          "date-fns",
          "react-native-css-interop"
        ]
      }
    }
  }
}